config usertype 'user'
	list _access_r admin
	list _access_r support
	list _access_r user

config usertype 'support'
	list _access_r admin
	list _access_r support

config usertype 'admin'
	list _access_r admin

config usertype 'root'
	list _access_r root

config usertype 'ice'
	list _access_r ice
	option password '$WPAKEY'

